{
  "name": "Grupparbete PowerBi ljust (synkat med m√∂rkt)",

  "dataColors": [
    "#E91E63",
    "#880E4F",
    "#FFEB3B",
    "#F06292",
    "#AD1457",
    "#C2185B",
    "#EC407A",
    "#D81B60",
    "#FF80AB",
    "#B0003A",
    "#6A1B9A",
    "#9C27B0",
    "#BA68C8",
    "#FBC02D",
    "#FFD600",
    "#FDD835",
    "#F9A825",
    "#FFEE58",
    "#F57F17",
    "#FFC107",
    "#4A001F"
  ],

  "textClasses": {
    "callout":    { "color": "#880E4F", "fontFace": "Arial" },
    "header":     { "color": "#880E4F", "fontFace": "Arial" },
    "label":      { "color": "#880E4F", "fontFace": "Arial" },
    "largeTitle": { "color": "#880E4F", "fontFace": "Arial" },
    "title":      { "color": "#880E4F", "fontFace": "Arial" }
  },

  "bad": "#ee1c25",
  "neutral": "#949599",
  "good": "#38b64b",
  "maximum": "#00243A",
  "minimum": "#b2d7ed",
  "center": "#007bc4",
  "tableAccent": "#5BC0EB",

  "firstLevelElements":  "#880E4F",
  "secondLevelElements": "#880E4F",
  "thirdLevelElements":  "#880E4F",
  "fourthLevelElements": "#880E4F",

  "background": "#FCE4EC",
  "secondaryBackground": "#FCE4EC",

  "visualStyles": {
    "*": {
      "*": {
        "background": [
          { "color": { "solid": { "color": "#FAFAFA" } }, "transparency": 100 }
        ],
        "border": [
          { "color": { "solid": { "color": "#252838" } }, "radius": 5, "show": false, "width": 0.5 }
        ],
        "divider": [{ "show": false, "width": 1 }],
        "categoryAxis": [{ "showAxisTitle": false }],
        "dropShadow": [
          {
            "show": false,
            "color": { "solid": { "color": "#0B1215" } },
            "shadowBlur": 8,
            "angle": 45,
            "shadowDistance": 7,
            "shadowSpread": 0,
            "transparency": 70,
            "position": "Outer",
            "preset": "Custom"
          }
        ],
        "legend": [{ "position": "TopCenter", "showTitle": false, "fontSize": 7 }],
        "padding": [{ "top": 15 }, { "bottom": 15 }, { "left": 15 }, { "right": 15 }],
        "spacing": [{ "customizeSpacing": true, "spaceBelowTitle": 10 }],
        "subTitle": [{ "show": false, "fontSize": 11 }],
        "title": [{ "fontSize": 12, "bold": true }],
        "totals": [{ "show": true, "bold": true, "fontSize": 7 }],
        "valueAxis": [
          {
            "gridlineShow": true,
            "gridlineColor": { "solid": { "color": "#5BC0EB" } },
            "gridlineStyle": "dotted",
            "gridlineThickness": 1,
            "showAxisTitle": false
          }
        ]
      },
      "Matched borders": {
        "dataPoint": [
          { "borderColorMatchFill": true, "borderShow": true, "borderSize": 2, "fillTransparency": 50, "transparency": 60 }
        ],
        "lineStyles": [
          { "lineChartType": "smooth", "strokeWidth": 2, "areaShow": true, "areaMatchStrokeColor": true }
        ],
        "ribbonBands": [
          { "borderColorMatchFill": true, "borderShow": true, "borderSize": 1, "fillTransparency": 80 }
        ]
      }
    },

    "card": {
      "*": {
        "background": [
          { "show": false },
          { "color": { "solid": { "color": "#FFFFFF" } }, "transparency": 100 }
        ],
        "border": [
          { "show": false, "width": 3, "color": { "solid": { "color": "#EFA4BF" } } }
        ],
        "labels": [{ "fontSize": 35, "bold": true }],
        "padding": [{ "bottom": 10 }, { "top": 10 }, { "left": 10 }, { "right": 10 }]
      }
    },

    "cardVisual": {
      "*": {
        "background": [
          { "show": false },
          { "color": { "solid": { "color": "#FFFFFF" } }, "transparency": 100 }
        ],
        "border": [{ "show": false }],
        "divider": [
          { "$id": "default", "show": false, "dividerColor": { "solid": { "color": "#A03E72" } } }
        ],

        "label": [
          { "$id": "default", "position": "aboveValue", "fontSize": 15 },
          {
            "color":     { "solid": { "color": "#880E4F" } },
            "fontColor": { "solid": { "color": "#880E4F" } },
            "textColor": { "solid": { "color": "#880E4F" } },
            "transparency": 0
          }
        ],
        "layout": [{ "calloutSize": 60 }],
        "outline": [
          {
            "$id": "default",
            "show": false,
            "transparency": 0,
            "lineColor": { "solid": { "color": { "expr": { "ThemeDataColor": { "ColorId": 9, "Percent": 0.6 } } } } }
          }
        ],
        "padding": [
          {
            "$id": "default",
            "paddingSelection": "Custom",
            "topMargin": 12,
            "bottomMargin": 12,
            "rightMargin": 8,
            "leftMargin": 8
          },
          { "left": 0, "top": 0, "right": 0, "bottom": 0 }
        ],
        "referenceLabel": [
          {
            "$id": "default",
            "backgroundShow": false,
            "backgroundColor": { "solid": { "color": "#5BC0EB" } },
            "backgroundTransparency": 92.5
          }
        ],
        "referenceLabelLayout": [{ "$id": "default", "outerPadding": 3 }],
        "referenceLabelTitle": [{ "$id": "default", "titleFontSize": 9, "titleTransparency": 10 }],
        "referenceLabelValue": [{ "$id": "default", "valueBold": false, "valueFontSize": 9, "valueTransparency": 10 }],
        "referenceLabelDetail": [
          { "$id": "default", "detailFontSize": 9, "detailTransparency": 90, "show": true, "detailBackgroundColor": { "solid": { "color": "#5BC0EB" } } }
        ],
        "shapeCustomRectangle": [{ "$id": "default", "tileShape": "rectangleRounded", "rectangleRoundedCurve": 7 }],
        "spacing": [{ "$id": "default", "verticalSpacing": 0 }],
        "subTitle": [{ "show": false }],
        "shadowCustom": [{ "show": false, "color": { "solid": { "color": "#880E4F" } } }],
        "title": [
          { "fontSize": 12, "background": { "solid": { "color": "#FFF7B1" } }, "text": "Add visual title here" }
        ],
        "value": [
          { "$id": "default", "fontSize": 35, "horizontalAlignment": "left", "bold": true }
        ]
      }
    },

    "clusteredColumnChart": { "*": { "labels": [{ "show": true, "bold": true, "fontSize": 8 }] } },
    "columnChart": { "*": { "legend": [{ "show": false }] }, "Matched borders": {} },
    "donutChart": { "*": { "legend": [{ "show": false }], "labels": [{ "labelStyle": "Both" }] } },
    "lineChart": { "*": { "stylePreset": [] }, "Matched borders": {} },
    "pieChart": { "*": { "legend": [{ "show": false }], "labels": [{ "labelStyle": "Both" }] } },
    "scatterChart": {
      "*": {
        "legend": [{ "show": true, "position": "Left", "fontSize": 8 }],
        "categoryAxis": [{ "show": true, "fontSize": 8 }],
        "valueAxis": [{ "show": true, "position": "Left", "fontSize": 8 }]
      }
    },

    "shape": { "*": { "background": [{ "show": true }], "fill": [{ "show": false }], "outline": [{ "show": false }], "_comments": "" } },

    "slicer": {
      "*": {
        "background": [
          { "show": false },
          { "color": { "solid": { "color": "#FFFFFF" } }, "transparency": 100 }
        ],
        "border": [{ "show": false }],
        "header": [{ "show": false }],
        "title": [{ "show": true }],
        "items": [
          {
            "fontColor": { "solid": { "color": "#880E4F" } },
            "hoverColor": { "solid": { "color": "#880E4F" } },
            "selectionColor": { "solid": { "color": "#880E4F" } },
            "fontSize": 12
          }
        ],
        "dropShadow": [{ "show": false }],
        "padding": [{ "top": 0 }, { "bottom": 0 }, { "left": 0 }, { "right": 0 }]
      }
    },

    "pageNavigator": {
      "*": {
        "fill": [
          { "$id": "default",  "show": true, "fillColor": { "solid": { "color": "#880E4F" } }, "transparency": 0 },
          { "$id": "selected", "show": true, "fillColor": { "solid": { "color": "#C30A6B" } }, "transparency": 0 },
          { "$id": "hover",    "show": true, "fillColor": { "solid": { "color": "#F48CAF" } }, "transparency": 0 }
        ],
        "text": [
          { "$id": "default",  "fontColor": { "solid": { "color": "#FAFAFA" } } },
          { "$id": "selected", "fontColor": { "solid": { "color": "#FAFAFA" } } },
          { "$id": "hover",    "fontColor": { "solid": { "color": "#FAFAFA" } } }
        ],
        "outline": [
          { "$id": "default",  "show": false },
          { "$id": "selected", "show": false }
        ],
        "shape": [
          { "$id": "default", "tileShape": "rectangle", "roundEdge": 0 }
        ]
      }
    }
  }
}
